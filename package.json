{
    "name": "ttt",
    "version": "1.6.32",
    "description": "ESLint Styling and Unit-Testing",
    "scripts": {
        "build": "tsc --build tsconfig.json",
        "lint": "npm run build && npx eslint .",
        "lint-fix": "npx eslint --fix .",
        "lint-debug": "npm run build && npx eslint --debug .",
        "pretest": "npm run build",
        "update-snapshots": "npm run build && node $(command -v jest) --config jest.config.js --injectGlobals --detectOpenHandles --update-snapshot",
        "prepublishOnly": "npm run test",
        "test": "node $(command -v jest) --config jest.config.js --injectGlobals --detectOpenHandles",
        "ts-setup": "npx tsc --init"
    },
    "license": "BSD-3-Clause",
    "author": {
        "name": "Jacob B. Sanders",
        "url": "https://github.com/iac-factory",
        "organization": true
    },
    "publishConfig": {
        "access": "public",
        "tag": "latest"
    },
    "devDependencies": {
        "@iac-factory/eslint-settings": "^1.7.36",
        "@jest/globals": "^28.1.0",
        "@jest/types": "^28.1.0",
        "@types/jest": "^28.1.3",
        "@types/mocha": "^9.1.1",
        "@types/node": "^17.0.40",
        "ansi-colors": "^4.1.3",
        "chokidar": "^3.5.3",
        "jest": "^28.1.1",
        "typescript": "^4.7.3"
    },
    "eslintConfig": {
        "extends": [
            ".eslintrc.json"
        ]
    }
}
